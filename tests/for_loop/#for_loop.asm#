	.section .text
	.globl	_start
_start:
.LFB0:
	pushq	%rbp
	movq	%rsp, %rbp
    
	movl	$0, -8(%rbp) # int max = 0
	movl	$0, -4(%rbp) # int i = 0
	jmp	.for_end
.for_start:
    
	addl	$1, -8(%rbp) # max++

	addl	$1, -4(%rbp) # i++
.for_end:
	cmpl	$100, -4(%rbp) 
	jl	.for)en              # i < 100
    
    movl -8(%rbp), %eax
    movl %eax, %edi
    movl $60, %eax
    syscall
